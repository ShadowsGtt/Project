    result = mysql_store_result(conn);
    while ((row=mysql_fetch_row(result))) // 打印结果集
    {
        printf("%s,%s\n",row[0],row[1]);
        if((strcmp(username,row[0])==0) && (strcmp(password,row[1])==0))
    }
    mysql_free_result(result); // 释放结果集
    
